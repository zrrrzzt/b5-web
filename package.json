{
  "name": "b5-web",
  "version": "1.9.1",
  "description": "Static site for big five personality tests",
  "main": "index.js",
  "scripts": {
    "test": "standard && nsp check && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "clear": "rm -rf .next && rm -rf out",
    "export": "next build && next export",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install",
    "deploy": "now --static out --name b5-web"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zrrrzzt/b5-web.git"
  },
  "keywords": [
    "big five",
    "personality test",
    "next.js"
  ],
  "author": "Geir GÃ¥sodden <geir.gasodden@pythonia.no> (https://github.com/zrrrzzt)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zrrrzzt/b5-web/issues"
  },
  "homepage": "https://github.com/zrrrzzt/b5-web#readme",
  "engines": {
    "node": ">= 10.0.0"
  },
  "now": {
    "engines": {
      "node": "10.0.0"
    }
  },
  "dependencies": {
    "b5-calculate-score": "1.0.4",
    "b5-johnson-120-ipip-neo-pi-r": "1.1.12",
    "b5-result-text": "1.3.0",
    "file-saver": "1.3.8",
    "jcb64": "1.1.2",
    "next": "6.0.2",
    "react": "16.3.2",
    "react-dom": "16.3.2",
    "react-easy-chart": "1.0.0"
  },
  "devDependencies": {
    "ava": "0.25.0",
    "coveralls": "3.0.1",
    "nsp": "3.2.1",
    "nyc": "11.8.0",
    "standard": "11.0.1"
  }
}
