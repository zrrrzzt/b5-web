{
  "name": "b5-web",
  "version": "2.0.6",
  "description": "Static site for big five personality tests",
  "main": "index.js",
  "scripts": {
    "test": "standard && ava",
    "test-audit": "standard && npm audit && ava",
    "test-offline": "standard && ava",
    "coverage": "nyc ava",
    "coveralls": "nyc ava && nyc report --reporter=lcov && cat coverage/lcov.info | coveralls",
    "standard-fix": "standard --fix",
    "dev": "next",
    "build": "next build",
    "start": "next start",
    "clear": "rm -rf .next && rm -rf dist",
    "export": "next build && next export -o dist",
    "refresh": "rm -rf node_modules && rm package-lock.json && npm install",
    "deploy": "now --prod"
  },
  "standard": {
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaFeatures": {
        "experimentalObjectRestSpread": true,
        "jsx": true
      },
      "sourceType": "module"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zrrrzzt/b5-web.git"
  },
  "keywords": [
    "big five",
    "personality test",
    "next.js"
  ],
  "author": "Geir GÃ¥sodden <geir.gasodden@pythonia.no> (https://github.com/zrrrzzt)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zrrrzzt/b5-web/issues"
  },
  "homepage": "https://github.com/zrrrzzt/b5-web#readme",
  "engines": {
    "node": ">= 12.14.1"
  },
  "dependencies": {
    "@alheimsins/b5-johnson-120-ipip-neo-pi-r": "4.0.0",
    "@alheimsins/b5-result-text": "1.7.5",
    "@alheimsins/bigfive-calculate-score": "1.0.3",
    "file-saver": "2.0.2",
    "jcb64": "2.0.2",
    "next": "9.2.1",
    "next-offline": "4.0.6",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-easy-chart": "1.0.0"
  },
  "devDependencies": {
    "ava": "3.1.0",
    "babel-eslint": "10.0.3",
    "coveralls": "3.0.9",
    "nyc": "15.0.0",
    "standard": "14.3.1"
  }
}
